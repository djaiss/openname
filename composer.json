{
    "name": "monicahq/touslesprenoms",
    "type": "project",
    "description": "Intranet for asynchronous teams.",
    "keywords": ["laravel", "intranet", "asynchronous"],
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "codezero/laravel-localizer": "^2.0",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^10.6",
        "laravel/tinker": "^2.8",
        "mauricius/laravel-htmx": "^0.5.0",
        "openai-php/laravel": "^0.8.0",
        "spatie/laravel-sitemap": "^7.0",
        "spatie/simple-excel": "^3.2"
    },
    "require-dev": {
        "amirami/localizator": "^0.12.1@alpha",
        "barryvdh/laravel-debugbar": "^3.9",
        "brianium/paratest": "^7.4",
        "fakerphp/faker": "^1.9",
        "larastan/larastan": "^2.7",
        "laravel/breeze": "^1.26",
        "laravel/dusk": "^7.11",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "psalm/plugin-laravel": "^2.7",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-ignition": "^2.0",
        "stichoza/google-translate-php": "^5.1",
        "tightenco/duster": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
          "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
